package Assignment;
import java.util.*;
public class program9 {
  public static void main(String args[]) {
	  Scanner sc = new Scanner(System.in);
	  System.out.println("Enter the Character");
	  char ch = sc.next().charAt(0);
	          if (ch >= 'a' && ch <= 'z') {
	              ch = (char)(ch - 32);
	          } else if (ch >= 'A' && ch <= 'Z') {
	              ch = (char)(ch + 32);
	          }
	          System.out.println("Output : " + ch);
	      }
}

